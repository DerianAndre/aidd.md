{
  "$schema": "https://raw.githubusercontent.com/nicegui-org/nicegui/main/nicegui/templates/capability.schema.json",
  "identifier": "default",
  "description": "Default capability for aidd.md Hub",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "shell:default",
    "shell:allow-open"
  ]
}
