      "env": {
        "AIDD_PROJECT_PATH": "C:/Archivos de proyecto/GitHub/DerianAndre/aidd.md"
      },
      "args": [
        "C:/Archivos de proyecto/GitHub/DerianAndre/aidd.md/mcps/mcp-aidd-engine/dist/index.js"
      ],
